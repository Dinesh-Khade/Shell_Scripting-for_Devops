# Script for auto backup with arg folder and timestamp 
#!/bin/bash



<< arg

This script takes backup of any destination path given in argument

./backup-with_arg <arg>
arg



Timestamp="$(date '+%y-%m-%d-%H-%M-%s')"


backup_dir="/home/ubuntu/crontab_auto-backups${Timestamp}_crontab_auto_backup.sh"

zip -r $backup_dir $1

echo "backup complete"

# Auto backup files every 1 min.
crontab_auto-backups
